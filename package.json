{
  "name": "tinyaiapi",
  "version": "1.0.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "ethereum": "jasmindreasond.x",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/index.js"
    }
  },
  "repository": "https://github.com/JasminDreasond/TinyAiApi.git",
  "author": "Yasmin Seidel (JasminDreasond)",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "jsonrepair": "^3.12.0",
    "object-hash": "^3.0.0"
  },
  "keywords": [
    "ai",
    "api",
    "llm",
    "chatbot",
    "gemini",
    "google-ai",
    "ai-client",
    "ai-session",
    "multi-provider",
    "ai-api",
    "api-client",
    "ai-conversation-manager",
    "ai-conversation",
    "llm-framework",
    "llm-client",
    "customizable",
    "extensible"
  ],
  "scripts": {
    "fix:prettier": "prettier --write ./src/*",
    "auto-build": "npm run build -- -w",
    "build": "tsc -p tsconfig.json",
    "build-all": "npm run build && npm run build-commonjs",
    "build-clean": "npm run clean && npm run build",
    "build-commonjs": "tsc --project tsconfig.cjs.json",
    "build-dist": "npm run build",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "prettier": "3.5.3",
    "typescript": "^5.8.3"
  }
}
